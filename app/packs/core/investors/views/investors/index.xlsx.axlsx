wb = xlsx_package.workbook

form_type, custom_field_names, custom_headers = get_form_type("Investor")

wb.add_worksheet(name: "Investors") do |sheet|
  # Create the header row
  sheet.add_row(%w[Name Tags Category City] + custom_headers)

  # Create entries for each item
  @investors.each do |inv|
    custom_field_values = get_custom_values(inv, form_type, custom_field_names)
    sheet.add_row [inv.investor_name, inv.tag_list, inv.category, inv.city] + custom_field_values
  end
end
