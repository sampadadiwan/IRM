        <% if model.pan_card.present? %>
            <% if model.pan_verified %>
              <span class="badge bg-success" type="button" 
                    data-bs-toggle="collapse" data-bs-target=".pan_verification_response">
                Verified
              </span>
            <% elsif model.pan_verification_status %>
              <span class="badge bg-danger" type="button" 
                    data-bs-toggle="collapse" data-bs-target=".pan_verification_response">
                  <%= model.pan_verification_status %>
              </span>              
            <% end %>
            <br>
            <small class="text-muted collapse pan_verification_response">
              <%= model.pan_verification_response if current_user  %>
            </small>
        <% else %>
            <span class="badge bg-warning">Not Uploaded</span>
        <% end %>