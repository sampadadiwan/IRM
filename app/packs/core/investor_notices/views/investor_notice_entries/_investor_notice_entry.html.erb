<tr id="<%= dom_id investor_notice_entry %>">
  <td>
    <%= investor_notice_entry.investor_notice.title %>
  </td>
  <td>
    <%= investor_notice_entry.investor.investor_name %>
  </td>
  <td>
    <%= display_boolean investor_notice_entry.active %>
  </td>
  <td>
    <%= link_to "Show" , investor_notice_entry , {class: "btn btn-primary" }%>
    <%= button_to "Delete", investor_notice_entry, method: :delete, class: "btn btn-danger", form_class: "deleteButton" %>  
  </td>
</tr>