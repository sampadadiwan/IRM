<%= form.fields_for :properties, OpenStruct.new(model.properties) do |builder| %>
    <% model.form_type.form_custom_fields.each do |field| %>
        <%= render "form_custom_fields/#{field.field_type.underscore}", 
                    field: field, f: builder %>
    <% end %>
<% end %>

<%= form.text_field :form_type_id, class: "form-control", hidden: true  %>
