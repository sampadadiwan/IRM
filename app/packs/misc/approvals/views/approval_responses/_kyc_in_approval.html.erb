<% if @approval_response && @approval.enable_approval_show_kycs? %>
    <p>
    <div>
        KYCs in this approval:
        <ol>        
        <% @approval_response.investor.investor_kycs.not_expired.each do |kyc| %>        
            <li>
                <%= kyc.full_name %>, 
                <%= kyc.PAN %>
            </li>        
        <% end %>
        </ol>
    </div>
    </p>
<% end %>