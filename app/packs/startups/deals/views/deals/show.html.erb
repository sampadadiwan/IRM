


<% if params[:kanban].present? %>
  <% deal_investor = @deal_investor || nil %>
  <% deal_activity = @deal_activity || nil %>
  <%= render "/deals/kanban", deal: @deal, deal_investors: @deal_investors, deal_investor: deal_investor, deal_activity: deal_activity %>
<% else %>
  <%= render "/deals/show", deal: @deal %>
<% end %>

