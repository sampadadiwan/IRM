<tr id="<%= dom_id stock_adjustment %>">
  <% unless params["portfolio_company_id"].present? %>
    <td>
      <%= stock_adjustment.portfolio_company.investor_name %>
    </td>
  <% end %>
  <td>
    <%= stock_adjustment.user.full_name %>
  </td>
  <td>
    <%= link_to stock_adjustment.investment_instrument %> 
  </td>
  <td>
    <%= stock_adjustment.adjustment %>
  </td>
  <td>
    <%= l stock_adjustment.created_at %>
  </td>
  <td>
    <%= stock_adjustment.notes %>
  </td>
</tr>
