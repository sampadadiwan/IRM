<%details ||= nil %>
<% if !details.blank? %>
<div class="col-xl-12">
    <div class="hero-content my-5 my-xl-0">

        <p
        class="fs-4 text-dark fw-semibold"
        data-aos="fade-up"
        data-aos-delay="600"
        data-aos-duration="1000"
        >
            <%= details.html_safe %>
        </p>


    </div>
</div>
<% end %>
