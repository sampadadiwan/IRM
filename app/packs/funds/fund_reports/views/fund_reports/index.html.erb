<%= render "/layouts/alerts" %>
<div class="d-sm-flex align-items-center justify-content-between mb-4">
  <h1 class="h3 mb-0 text-gray-800">Fund Reports</h1>
  <%= link_to "New Fund Report", new_fund_report_path, {class: "btn btn-primary"}  %>
</div>
<div class="table-responsive" data-controller="datatable">
  <table id="fund_reports" class="table table-bordered dataTable">
    <thead>
      <tr>
        <th>
          Name
        </th>
        <th>
          Name Of Scheme
        </th>
        <th>
          Start Date
        </th>
        <th>
          End Date
        </th>
        <th>
        </th>
      </tr>
    </thead>
    <tbody>
      <% @fund_reports.each do |fund_report| %>
        <%= render fund_report %>
      <% end %>
    </tbody>
  </table>
</div>
