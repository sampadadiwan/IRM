

<%= render "/layouts/filter", associations: [:capital_commitment, :allocation_run, :fund_formula], include_template: "/account_entries/group_filter", hidden_fields: {time_series: params[:time_series], pivot: params[:pivot], group_by: params[:group_by], hide_parent: params[:hide_parent], hide_commitment: params[:hide_commitment], parent_in_key: params[:parent_in_key]} %>

<%= render ReportingComponent.new do %>
  <%= turbo_frame_tag "account_entries_frame", target: "_top" do %>
    <% if params[:ag].present? %>
      <%= render "/account_entries/index_ag", account_entries: @account_entries, 
          fund: nil, lazy_load_data: false, data_source: nil %>
    <% elsif params[:time_series].present? %>
      <%= render "/account_entries/time_series" %>
    <% elsif params[:pivot].present? %>
      <%= render "/account_entries/pivot" %>
    <% else %>
      <%= render "/account_entries/index", account_entries: @account_entries, 
          fund: @fund, lazy_load_data: false, data_source: nil %>
    <% end %>
  <% end %>
<% end %>