<tr id="<%= dom_id fund_formula %>">
  <td>
    <%= fund_formula.sequence %>
  </td>
  <td>
    <%= fund_formula.name %>
  </td>
  <td>
    <%= fund_formula.rule_type %>
  </td>
  <td>
    <%= fund_formula.formula %>
  </td>
  <td>
    <%= fund_formula.entry_type %>
  </td>
  <td>
    <%= display_boolean fund_formula.roll_up %>
  </td>
  <td>
    <%= display_boolean fund_formula.enabled %>
  </td>
  <td>
    <%= link_to "Show", fund_formula , {class: "btn btn-outline-primary"}%>
    <%= link_to "Edit", edit_fund_formula_path(fund_formula) , {class: "btn btn-outline-success"}%>
    <%= link_to "Run Formula", allocate_form_fund_path(id: fund_formula.fund_id, 
                fund_formula_id: fund_formula.id), class: "btn btn-outline-success" if params[:debug].present? %> 
  </td>
</tr>
