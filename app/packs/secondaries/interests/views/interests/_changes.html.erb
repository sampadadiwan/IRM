<table class="table table-striped table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Version</th>
      <th>Key</th>
        <th>Old Value</th>
        <th>New Value</th>
      <th>Modified By</th>
      <th>Modified At</th>
    </tr>
  </thead>
  <tbody>
    <% interest.audits.each do |audit| %>
        <% audit.audited_changes.each do |key, value| %>
            <% if ["price", "quantity"].include?(key) && value[0] != value[1] %>
                <tr>
                    <td><%= audit.version %></td>
                    <td><%= key.humanize %></td>
                    <td><%= value[0] %></td>
                    <td><%= value[1] %></td>
                    <td><%= audit.user&.name %></td>
                    <td><%= audit.created_at.strftime("%d %b %Y %H:%M:%S") %></td>
                </tr>
            <% end %>
        <% end %>
      
    <% end %>
  </tbody>
</table>