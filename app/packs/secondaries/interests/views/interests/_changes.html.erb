<table class="table table-striped table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th><%= t('secondaries_actions.version') %></th>
      <th><%= t('secondaries_actions.key') %></th>
        <th><%= t('secondaries_actions.old_value') %></th>
        <th><%= t('secondaries_actions.new_value') %></th>
      <th><%= t('secondaries_actions.modified_by') %></th>
      <th><%= t('secondaries_actions.modified_at') %></th>
    </tr>
  </thead>
  <tbody>
    <% interest.audits.includes(:user).each do |audit| %>
        <% audit.audited_changes.each do |key, value| %>
            <% if ["price", "quantity", "short_listed_status"].include?(key) && value[0] != value[1] %>
                <tr>
                    <td><%= audit.version %></td>
                    <td><%= key.humanize %></td>
                    <td><%= value[0] %></td>
                    <td><%= value[1] %></td>
                    <td><%= audit.user&.name %></td>
                    <td><%= audit.created_at.strftime("%d %b %Y %H:%M:%S") %></td>
                </tr>
            <% end %>
        <% end %>
      
    <% end %>
  </tbody>
</table>