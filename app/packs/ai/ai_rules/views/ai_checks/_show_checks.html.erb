<% if params[:ai_checks].present? %>

    <% 
        model_class = controller_name.classify.constantize
        model = model_class.find(params[:id])
    %>

    <%= turbo_frame_tag "ai_checks_frame", target: "_top", loading: :lazy, src: ai_checks_path(owner_id: model.id, owner_type: model_class, rule_type: params[:rule_type]) do %>
        <%= render "/layouts/loading" %>
    <% end %>

<% end %>
