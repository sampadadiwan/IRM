<tr id="document_<%= document.id %>">
  <td>
      <%= link_to document.name, document_path(document.id) %>
  </td>  
  <td>
    <%= document&.folder_full_path %>
  </td>
  <td class="hideMobP">
      <%= raw document.tag_list.map{|tag| link_to(tag, search_documents_path(query: tag))}.join(", ") %>
  </td>  
  <td class="hideMobP">
    <%= document.created_at.is_a?(Time) ? l(document.created_at) : l(Time.parse(document.created_at)) %>
  </td>  

</tr>
