<p style="color: green"><%= notice %></p>

<h1 class="h3 mb-0 text-gray-800">Users</h1>

<table id="users" class="table table-bordered dataTable">

  <tr>
    <th>
      First name
    </th>

    <th>
      Last name
    </th>

    <th>
      Role
    </th>
  </tr>


  <% @users.each do |user| %>
    <%= render user %>    
  <% end %>
</table>
