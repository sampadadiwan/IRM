<%= render "/layouts/alerts" %>

<% if current_user.entity %>
    <% if current_user.curr_role.to_sym == :startup %>
        <%= render "cxo_dashboard" %>
    <% elsif current_user.curr_role.to_sym == :investor %>
        <%= render "vc_dashboard" %>
    <% elsif current_user.entity.entity_type == "Holding" %>
        <%= render "holding_dashboard" %>
    <% elsif current_user.entity.entity_type == "Holding" %>
        <%= render "advisor_dashboard" %>
    <% end %>
<% end %>