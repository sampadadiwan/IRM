
<tr id="<%= dom_id entity %>">
<td>
    <%= link_to entity.name, entity if !vc_view %>
    <%= link_to entity.name, investor_view_entity_path(entity) if vc_view %>
  </td>  
<td>
    <%= entity.category %>
</td>  
<td>
    <%= entity.entity_type %>
</td>  
<td>
    <%= link_to "Documents", documents_path(entity_id: entity.id) , {class: "btn btn-primary"}%>
    <%= link_to "Investments", investments_path(entity_id: entity.id) , {class: "btn btn-success"}%>
</td>  

</tr>
