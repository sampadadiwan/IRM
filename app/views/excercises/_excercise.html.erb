<tr id="<%= dom_id excercise %>">
  <td>
    <%= excercise.entity.name %>
  </td>
  <td>
    <%= excercise.user.full_name %>
  </td>
  <td>
    <%= excercise.option_pool.name %>
  </td>
  <td>
    <%= excercise.quantity %>
  </td>
  <td>
    <%= excercise.price %>
  </td>
  <td>
    <%= excercise.amount %>
  </td>
  
  <td>
    <%= excercise.approved ? "Yes" : "No" %>
  </td>
  <td>
    <%= link_to "Show" , excercise , {class: "btn btn-primary" }%>
    <% label = excercise.approved ?  "UnApprove" : "Approve" %>
    <%= button_to label , approve_excercise_path(excercise), method: :patch, class: "btn btn-success" , form_class: "deleteButton" if policy(excercise).approve? %>
  </td>
</tr>